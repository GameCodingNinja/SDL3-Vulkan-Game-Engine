<objectDataList2D>

    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
    <default  name="">
        <visual>
            <texture count="0" file="" compressed="false"/>
            <mesh genType="quad" file="" >
                <spriteSheet defIndex="0" glyphCount="0" columns="0" formatCodeOffset="0" loadAllGlyphs="false" file=""/>
                <scaledFrame thicknessWidth="" thicknessHeight="" centerQuad="" frameBottom=""/>
            </mesh>
            <color r="1" g="1" b="1" a="1"/>
            <pipeline id="2d_quad"/>
        </visual>
        <physics world="(game)">
            <body type="" linearDamping="0" angularDamping="0" fixedRotation="false"/>
            <fixtureList>
                <fixture shape="" radius="0" chainLoop="false" friction="0.2" density="0.2" restitution="0.2">
                    <!-- <collisionFilter categoryBits="" maskBits="" groupIndex=""/> -->
                    <!-- <vertList>
                    vert list for polygon shapes - max of 8
                    Box2D polygons are defined using Counter Clockwise Winding (CCW)
                    <vert x="" y=""/>
                    </vertList> -->
                    <sizeMod top="0" bottom="0" left="0" right="0"/>
                </fixture>
            </fixtureList>
        </physics>
        <size width="0" height="0"/>
    </default>
    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
  
    <objectList>
        
        <object name="circle_green">
            <visual>
                <texture file="data/textures/run/circle_green.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="circle" radius="48" density="0.3" restitution="0.6"/>
                </fixtureList>
            </physics>
        </object>
    
        <object name="circle_blue">
            <visual>
                <texture file="data/textures/run/circle_blue.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="circle" radius="48" density="0.2" restitution="0.4"/>
                </fixtureList>
            </physics>
        </object>
        
        <object name="circle_red">
            <visual>
                <texture file="data/textures/run/circle_red.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="circle" radius="48" density="0.1" restitution="0.3"/>
                </fixtureList>
            </physics>
        </object>
    
        <object name="square_red">
            <visual>
                <texture file="data/textures/run/square_red.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.1" restitution="0.2">
                        <sizeMod top="-4" bottom="-3" left="-4" right="-3"/>
                    </fixture>
                </fixtureList>
            </physics>
        </object>
        
        <object name="triangle_green">
            <visual>
                <texture file="data/textures/run/triangle_green.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.3" restitution="0.6">
                        <vertList>
                            <vert x="50" y="5"/>
                            <vert x="5" y="94"/>
                            <vert x="94" y="94"/>
                        </vertList>
                    </fixture>
                </fixtureList>
            </physics>
        </object>
    
        <object name="triangle_blue">
            <visual>
                <texture file="data/textures/run/triangle_blue.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.2" restitution="0.4">
                        <vertList>
                            <vert x="50" y="5"/>
                            <vert x="5" y="94"/>
                            <vert x="94" y="94"/>
                        </vertList>
                    </fixture>
                </fixtureList>
            </physics>
        </object>

        <object name="peg">
            <visual>
                <texture count="2" file="data/textures/run/peg%d.png"/>
            </visual>
            <physics>
                <body type="static"/>
                <fixtureList>
                    <fixture shape="circle" radius="17"/>
                </fixtureList>
            </physics>
        </object>

    </objectList>

</objectDataList2D>

