<?xml version="1.0"?>
<objectDataList2D>

    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
    <default>
        <visual>
            <mesh genType="quad"/>
            <color r="1" g="1" b="1" a="1"/>
            <pipeline id="2d_quad"/>
        </visual>
    </default>
    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
  
    <objectList>

        <object name="background">
            <visual>
                <texture file="data/textures/titleScreen/title_background.png"/>
            </visual>
        </object>

        <object name="title">
            <visual>
                <texture file="data/textures/titleScreen/title.png"/>
            </visual>
        </object>

        <object name="circle_green">
            <visual>
                <texture file="data/textures/level/circle_green.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="circle" radius="48" density="0.3" restitution="0.6"/>
                </fixtureList>
            </physics>
        </object>
    
        <object name="circle_blue">
            <visual>
                <texture file="data/textures/level/circle_blue.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="circle" radius="48" density="0.2" restitution="0.4"/>
                </fixtureList>
            </physics>
        </object>
        
        <object name="circle_red">
            <visual>
                <texture file="data/textures/level/circle_red.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="circle" radius="48" density="0.1" restitution="0.2"/>
                </fixtureList>
            </physics>
        </object>
    
        <object name="square_green">
            <visual>
                <texture file="data/textures/level/square_green.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.3" restitution="0.6">
                        <sizeMod top="-4" bottom="-3" left="-4" right="-3"/>
                    </fixture>
                </fixtureList>
            </physics>
        </object>

        <object name="square_blue">
            <visual>
                <texture file="data/textures/level/square_blue.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.2" restitution="0.4">
                        <sizeMod top="-4" bottom="-3" left="-4" right="-3"/>
                    </fixture>
                </fixtureList>
            </physics>
        </object>

        <object name="square_red">
            <visual>
                <texture file="data/textures/level/square_red.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.1" restitution="0.2">
                        <sizeMod top="-4" bottom="-3" left="-4" right="-3"/>
                    </fixture>
                </fixtureList>
            </physics>
        </object>
        
        <object name="triangle_green">
            <visual>
                <texture file="data/textures/level/triangle_green.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.3" restitution="0.6">
                        <vertList>
                            <vert x="50" y="5"/>
                            <vert x="5" y="94"/>
                            <vert x="94" y="94"/>
                        </vertList>
                    </fixture>
                </fixtureList>
            </physics>
        </object>
    
        <object name="triangle_blue">
            <visual>
                <texture file="data/textures/level/triangle_blue.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.2" restitution="0.4">
                        <vertList>
                            <vert x="50" y="5"/>
                            <vert x="5" y="94"/>
                            <vert x="94" y="94"/>
                        </vertList>
                    </fixture>
                </fixtureList>
            </physics>
        </object>

        <object name="triangle_red">
            <visual>
                <texture file="data/textures/level/triangle_red.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="dynamic"/>
                <fixtureList>
                    <fixture shape="polygon" density="0.1" restitution="0.2">
                        <vertList>
                            <vert x="50" y="5"/>
                            <vert x="5" y="94"/>
                            <vert x="94" y="94"/>
                        </vertList>
                    </fixture>
                </fixtureList>
            </physics>
        </object>

        <object name="peg">
            <visual>
                <texture count="2" file="data/textures/level/peg%d.png"/>
                <color r="1" g="1" b="1" a="0.5"/>
            </visual>
            <physics world="(game)">
                <body type="static"/>
                <fixtureList>
                    <fixture shape="circle" radius="17"/>
                </fixtureList>
            </physics>
        </object>
        
    </objectList>

</objectDataList2D>

