<strategy>

    <!-- Node name will be used for the object name look up if one isn't defined -->
    <node name="cube" defaultGroup="(cube)">
        
        <sprite>
            <position x="0" y="0" z="0"/>
            <scale x="3" y="3" z="3"/>
            <scriptList>
                <script rotate="State_RotateCube" group="(state)"/>
            </scriptList>
        </sprite>

    </node>

</strategy>
